<%= form_for @print do |f| %>
<div class="ui form">

</div>
<div class="field">
  <div class="">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="">
    <%= f.label :quantity %>
    <%= f.text_field :quantity %>
  </div>
</div>

<div class="field">
  <div class="">
    <%= f.label :contact_email %>
    <%= f.text_field :contact_email %>
  </div>
</div>

<div class="field">
  <div class="">
    <%= f.label :contact_phone %>
    <%= f.text_field :contact_phone %>
    <%= f.hidden_field :order_state, :value => 0 %>
  </div>
</div>

<%= f.submit "Submit Print Order", class: "erb-btn", :controller => 'prints', :action => 'user_create' %>
<input type="button" value="Go Back" class="back-btn erb-btn">
<% end %>
